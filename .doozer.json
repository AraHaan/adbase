{
  "targets": {
    "xenial-amd64": {
      "buildenv": "xenial-amd64",
      "builddeps": ["gcc-4.8", "g++-4.8", "libgtest-dev", "cmake"],
      "buildcmd": ["cd /usr/src/gtest", "cmake .", "make", "cp libgtest*.a /usr/local/lib", "cd - && mkdir build", "cmake -DCMAKE_BUILD_TYPE=Debug -DM32=0 -DUSEKAFKA=0 -DADINFVERSION= -DADINFSOVERSION= -DDEV=1 -DCMAKE_CXX_COMPILER=/usr/bin/g++-4.8 .."]
    }
  }
}
